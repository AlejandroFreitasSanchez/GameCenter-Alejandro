<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="900.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.gamecenter.AddGameController">
   <children>
      <Pane prefHeight="900.0" prefWidth="1300.0" style="-fx-background-color: darkslateblue;">
         <children>
            <Label layoutX="270.0" layoutY="184.0" prefHeight="80.0" prefWidth="225.0" text="Nombre" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="36.0" />
               </font>
            </Label>
            <Label layoutX="270.0" layoutY="354.0" prefHeight="80.0" prefWidth="225.0" text="Descripción" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="36.0" />
               </font>
            </Label>
            <Label layoutX="270.0" layoutY="505.0" prefHeight="80.0" prefWidth="225.0" text="Precio" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="36.0" />
               </font>
            </Label>
            <TextField fx:id="nameField" layoutX="740.0" layoutY="195.0" prefHeight="80.0" prefWidth="225.0">
               <font>
                  <Font size="36.0" />
               </font></TextField>
            <TextField fx:id="descField" layoutX="740.0" layoutY="359.0" prefHeight="80.0" prefWidth="225.0">
               <font>
                  <Font size="36.0" />
               </font></TextField>
            <TextField fx:id="priceField" layoutX="740.0" layoutY="505.0" prefHeight="80.0" prefWidth="225.0">
               <font>
                  <Font size="36.0" />
               </font></TextField>
            <Button fx:id="sendButton" layoutX="624.0" layoutY="699.0" mnemonicParsing="false" onAction="#enviar" prefHeight="80.0" prefWidth="225.0" text="Enviar">
               <font>
                  <Font name="System Bold" size="36.0" />
               </font>
            </Button>
            <Button fx:id="goBackButton" layoutX="54.0" layoutY="45.0" mnemonicParsing="false" onAction="#volverAtras" prefHeight="80.0" prefWidth="225.0" text="Atrás">
               <font>
                  <Font name="System Bold" size="36.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</AnchorPane>
